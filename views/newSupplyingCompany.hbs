<form action="/sharingmaster/new/supplyingcompany" method="POST">
  Supplying Company Name:<br><br>
  <input type="text" name="supplyingcompanyname">
  <br><br><br>
  Selling company<br><br>
  <select name="sellingcompanyid">
    {{#each data}}
      <option value="{{this.id}}">{{this.name}}</option>
    {{/each}}
  </select>
  <br><br><br>
  Currency<br><br>
  <input type="text" name="currency">
  <br><br><br>
  <input type="submit" value="Add">
</form> 